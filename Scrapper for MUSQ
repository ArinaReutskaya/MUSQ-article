pip install selenium pandas webdriver-manager

from selenium import webdriver
from selenium.webdriver.chrome.service import Service
from selenium.webdriver.common.by import By
from selenium.webdriver.chrome.options import Options
from webdriver_manager.chrome import ChromeDriverManager
import pandas as pd
import time

# Konfiguracja przeglądarki
options = Options()
driver = webdriver.Chrome(service=Service(ChromeDriverManager().install()), options=options)

# Przejście do strony MUSQ history
url = "https://finance.yahoo.com/quote/MUSQ/history?period1=1688774400&period2=1746641919"
driver.get(url)
time.sleep(5)  

# Znalezienie wierszy tabeli
rows = driver.find_elements(By.XPATH, '//table[contains(@data-test,"historical-prices")]/tbody/tr')

data = []

for row in rows:
    try:
        cols = row.find_elements(By.TAG_NAME, 'td')
        if len(cols) >= 5:
            date = cols[0].text
            close = cols[4].text.replace(',', '')
            data.append({"Date": date, "Close": close})
    except:
        continue  # pomijamy np. puste wiersze lub komunikaty

# Zapis do CSV
df = pd.DataFrame(data)
df["ETF"] = "MUSQ"
df.to_csv("musq_prices.csv", index=False)
print("Zapisano musq_prices.csv")

driver.quit()
